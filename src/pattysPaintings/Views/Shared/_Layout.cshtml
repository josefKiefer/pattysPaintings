<!DOCTYPE html>
<html lang="en">
<head>
    <title>Patricia Kiefer</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css" integrity="sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M" crossorigin="anonymous">
</head>



<body class="bg-light">
    <div class="container text-center">
        <h1 class="display-1">Patricia Kiefer</h1>
        <p class="lead">Paintings made by Patricia Kiefer and Website made by Jose</p>
        <ul class="nav justify-content-center">
            <li class="nav-item">
                <li>@Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })</li>
            </li>
            <li class="nav-item">
                <li>@Html.ActionLink("Gallery", "Detail2", "patty", new { area = "" }, new { @class = "navbar-brand" })</li>
            </li>
            <li class="nav-item">
                <li>@Html.ActionLink("About", "About", "patty", new { area = "" }, new { @class = "navbar-brand" })</li>
            </li>
            <li class="nav-item">
                <li>@Html.ActionLink("Contact", "Detail", "patty", new { area = "" }, new { @class = "navbar-brand" })</li>
            </li>
        </ul>
        

    </div>
</body>



<div class="container body-content">
    @RenderBody() <!--render body method tells mvc where we want content from view-->
    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year - Kiefer Productons</p>
    </footer>
</div>
</html>